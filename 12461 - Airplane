#include <cstdio>

using namespace std;

int main()
{
	#ifdef DEBUG
	freopen("input.in", "r", stdin);
	#endif

	short n;
	while(scanf("%hi", &n) && n != 0)
		printf("1/2\n");
	return 0;
}
